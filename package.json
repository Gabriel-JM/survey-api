{
  "name": "survey-api",
  "version": "2.5.5",
  "description": "",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node dist/main/server",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "dev": "npm run build && docker-compose up",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.ts",
    "test:int": "npm test -- --watch -c jest-integration.config.ts",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage --silent",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabriel-JM/survey-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gabriel-JM/survey-api/issues"
  },
  "homepage": "https://github.com/Gabriel-JM/survey-api#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mongodb": "^3.6.18",
    "@types/node": "^15.12.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^7.28.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^4.0.6",
    "jest": "^27.2.4",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^3.6.9",
    "nodemon": "^2.0.12",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.6.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
